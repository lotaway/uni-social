{
    "name": "uni-social",
    "version": "0.0.1",
    "description": "",
    "author": "lw",
    "scripts": {
        "build": "npm run build:h5",
        "build:app": "uni build -p app",
        "build:app-android": "uni build -p app-android",
        "build:app-ios": "uni build -p app-ios",
        "build:custom": "uni build -p",
        "build:h5": "uni build",
        "build:h5:ssr": "uni build --ssr",
        "dev": "npm run dev:h5",
        "dev:app": "uni -p app",
        "dev:app-android": "uni -p app-android",
        "dev:app-ios": "uni -p app-ios",
        "dev:custom": "uni -p",
        "dev:h5": "uni",
        "dev:h5:ssr": "uni --ssr",
        "test": "npm run test:h5",
        "test:h5": "cross-env UNI_PLATFORM=h5 jest -i --detectOpenHandles",
        "test:android": "cross-env UNI_PLATFORM=app UNI_OS_NAME=android jest -i",
        "test:ios": "cross-env UNI_PLATFORM=app UNI_OS_NAME=ios jest -i"
    },
    "devDependencies": {
        "@dcloudio/types": "^3.3.3",
        "babel-plugin-component": "^1.1.1",
        "jest": "^29.6.4",
        "pug": "^3.0.2",
        "reflect-metadata": "^0.1.13",
        "sass": "^1.55.0",
        "typescript": "^4.8.3",
        "vite": "^4.4.9",
        "vk-uview-ui": "^1.4.0",
        "vue": "^3.2.39",
        "vue-i18n": "^9.1.9",
        "vuex": "^4.0.2",
        "vuex-module-decorators": "^2.0.0"
    },
    "license": "ISC"
}
